# THIS FILE SERVES AS A BASIS TO A SHELLRC
# WE RECOMMEND YOU TO ADD YOUR OWN COMMANDS TO ~/.shellptrc
# IF NEEDED, YOU CAN STILL DELETE OR MODIFY HERE

# Init file that is printed out at start of shell
# Modify it as you wish
pff .shellptstart

export DESKTOP='${HOME}/Desktop'
export BUREAU='${HOME}/Bureau'

export LOWERALPHA=abcdefghijklmnopqrstuvwxyz
export UPPERALPHA=ABCDEFGHIJKLMNOPQRSTUVWXYZ
export NUM=0123456789
export ALPHA=${LOWERALPHA}
ALPHA+=${UPPERALPHA}
export ALPHANUM=$ALPHA
ALPHANUM+=$NUM

alias *='echo *'
alias *c='echo *.c'
alias *cpp='echo *.cpp'
alias *h='echo *.h'
alias *o='echo *.o'
alias .*='echo .*'
alias ?='echo ?'

alias .='cd .'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias cd..='cd ..'

alias -='cd $OLDPWD'
alias cds='cd srcs'
alias cdb='cd bonus'
alias cdm='cd mandatory'

alias mkdir='mkdir -pv'
alias rmrf='rm -rf'

alias _='sudo'

alias c=clear
alias e=exit
#alias h=history
alias q=exit

# tres importants
alias emacs=vim
alias vi=vim
alias nano=vim

alias l='ls -lah'
alias l.='ls -d .* --color=auto'
alias la='ls -lAh'
alias ll='ls -lh'
alias ls='ls --color=tty'
alias ks='echo "you meant ls ?"; ls'

